{{!< default}}


    <div class="content-wrap  page-post-detail">
        <div id="content" class="content">
            <div id="post" class="posts-expand fadeInDown">
                <article class="post post-type-normal" itemscope="" itemtype="http://schema.org/Article">
                    <header class="post-header">
                        <h1 class="post-title" itemprop="name headline"></h1>
                    </header>
                    <div class="post-body" itemprop="articleBody">
                        <br><br><br><br><br>
                        <img itemprop="image" height="20" width="200" src="http://huliujia.com/content/images/2018/09/image-1.png" alt="Email">
                    </div>
                </article>
            </div>
        </div>

    </div>


 <aside id="sidebar" class="sidebar" style="display: block; width: 240px;">
     <div class="sidebar-inner">
        {{> sidebar}}
     </div>
 </aside>